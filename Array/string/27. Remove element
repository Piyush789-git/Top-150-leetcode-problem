class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int k;
        int m  =nums.size();
        for(int i=0;i<m;i++){
           
            if(nums[i]==val){
                nums.erase(remove(nums.begin(), nums.end(), val), nums.end());

             k=nums.size();
               

            }
            return k;
            

        }
   return 0;
 }
 
};
